{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["prettier"],

  "rules": {
    "prettier/prettier": "error",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }]
  }
}
